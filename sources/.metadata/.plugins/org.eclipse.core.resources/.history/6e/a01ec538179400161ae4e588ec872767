package lista_agregacao;

public class Teclado {

	private int teclas;
	private String mapa;
	private String marca;
	private String modelo;
	private boolean semFio;

	public Teclado(int teclas, String mapa, String marca, String modelo, boolean semFio) {
		this.teclas = teclas;
		this.mapa = mapa;
		this.marca = marca;
		this.modelo = modelo;
		this.semFio = semFio;
	}
	
	
}
