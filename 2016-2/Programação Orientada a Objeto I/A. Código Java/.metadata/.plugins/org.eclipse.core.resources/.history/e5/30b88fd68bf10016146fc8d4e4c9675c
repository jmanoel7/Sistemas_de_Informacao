public class CDPlayer implements Player {

	private String marca;
	private String modelo;
    private boolean tocando;
    private boolean fimFaixa;
    private boolean inicioFaixa;
    private int faixa;
    private boolean reverso;

	public CDPlayer(String marca, String modelo) {
		this.marca = marca;
		this.modelo = modelo;
        this.inicioFaixa = true;
        this.faixa = 1;
        this.fimFaixa = false;
        this.tocando = false;
        this.reverso = false;
	}

	
	/* (non-Javadoc)
	 * @see lista_interfaces.Player#play()
	 */
	@Override
	public void play() {

	}

	/* (non-Javadoc)
	 * @see lista_interfaces.Player#stop()
	 */
