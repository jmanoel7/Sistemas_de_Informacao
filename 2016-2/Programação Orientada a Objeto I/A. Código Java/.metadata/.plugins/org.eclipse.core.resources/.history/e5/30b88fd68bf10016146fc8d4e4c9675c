	public void stop() {
		// TODO Auto-generated method stub

	}

	/* (non-Javadoc)
	 * @see lista_interfaces.Player#pause()
	 */
	@Override
	public void pause() {
		// TODO Auto-generated method stub

	}

	/* (non-Javadoc)
	 * @see lista_interfaces.Player#reverse()
	 */
	@Override
	public void reverse() {
		// TODO Auto-generated method stub

	}

}
